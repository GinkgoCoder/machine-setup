export SYSTEMD_EDITOR=vim

fpath+=($HOME/.oh-my-zsh/plugins/pure $HOME/.oh-my-zsh/plugins/tmux)
fpath+=${ZSH_CUSTOM:-${ZSH:-~/.oh-my-zsh}/custom}/plugins/zsh-completions/src

autoload -U promptinit; promptinit
prompt pure

plugins+=(zsh-completions zsh-autosuggestions zsh-syntax-highlighting docker nvm npm tmux)

source $ZSH/oh-my-zsh.sh

